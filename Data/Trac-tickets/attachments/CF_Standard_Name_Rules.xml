<?xml version="1.0"?>
<standard_name_rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="cf-standard-name-table-1.1.xsd">
   <title>CF Standard Name: Inferred Rules</title>
   <version_number>37</version_number>
   <last_modified>2016-11-16T13:56:56Z</last_modified>
   <institution>National Centre for Atmospheric Science</institution>
   <contact>support@ceda.ac.uk</contact>
   <rule id="atmosphere_convective_inhibition.requiredCoord">
      <description>PROBABLY NEEDS A VERTICAL COORDINATE AS WELL?</description>
      <target>atmosphere_convective_inhibition</target>
      <requiredCoordinate>original_air_pressure_of_lifted_parcel</requiredCoordinate>
   </rule>
   <rule id="moisture_content_of_soil_layer.boundAxis">
      <description>The soil layer must be described by a bounds attribute on a vertical coordinate.</description>
      <target>moisture_content_of_soil_layer</target>
      <requiredBoundAxis>Z</requiredBoundAxis>
   </rule>
   <rule id="region.charOrFlag">
      <description>When the standard name region is used, the terms must come from the standardized region list.</description>
      <target>region</target>
      <charOrFlagIn>CF Standard Region</charOrFlagIn>
   </rule>

</standard_name_rules>
